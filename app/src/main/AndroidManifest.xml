<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sqj.cpsc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logolauncher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logolauncher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.sqj.cpsc.AllCategory"></activity>
        <activity android:name="com.sqj.cpsc.CustomerActivity"></activity>
        <activity android:name="com.sqj.cpsc.loginActivity" />

        <activity android:name="com.sqj.cpsc.MainActivity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>